<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ListLeagueServlet</servlet-name>
		<display-name>ListLeagueServlet</display-name>
		<description></description>
		<servlet-class>comm.example.view.ListLeagueServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddLeagueServlet</servlet-name>
		<display-name>AddLeagueServlet</display-name>
		<description></description>
		<servlet-class>comm.example.controller.AddLeagueServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListLeagueServlet</servlet-name>
		<url-pattern>/list_leagues.view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddLeagueServlet</servlet-name>
		<url-pattern>/add_league.do</url-pattern>
	</servlet-mapping>
	<welcome-file-list>

		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>
